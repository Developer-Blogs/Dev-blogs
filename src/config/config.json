{
  "site": {
    "title": "Dev Blogs",
    "base_url": "https://developersblogs.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo.png",
    "logo_darkmode": "/images/logo-darkmode.png",
    "logo_width": "150",
    "logo_height": "30",
    "logo_text": "Developer blogs"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
    "default_theme": "system",
    "pagination": 12,
    "summary_length": 200,
    "blog_folder": "blog",
    "default_language": "en",
    "disable_languages": ["fr"],
    "default_language_in_subdir": false
  },

  "params": {
    "contact_form_action": "#",
    "copyright": "© 2025 Developer Blogs. All rights open-sourced.<br>Built with ❤️ by the KHC community"
  },

  "navigation_button": {
    "enable": true,
    "label": "Get Started",
    "link": "https://github.com/Snakes-n-Networks/Dev-blogs.git"
  },

  "disqus": {
    "enable": true,
    "shortname": "themefisher-template",
    "settings": {}
  },

  "metadata": {
    "meta_author": "ekrishnachaitanya2004",
    "meta_image": "/images/og-image.png",
    "meta_description": "Open Source Developers blogs"
  }
}
